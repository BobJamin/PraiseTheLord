<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="com.bobjamin.kratosplugin.ui.KratosToolWindowMetric">
  <grid id="header" binding="metric" layout-manager="BorderLayout" hgap="10" vgap="-1">
    <constraints>
      <xy x="0" y="0" width="223" height="51"/>
    </constraints>
    <properties>
      <background color="-12828863"/>
    </properties>
    <border type="none"/>
    <children>
      <grid id="header-wrapper" binding="metricWrapper" layout-manager="GridLayoutManager" row-count="1" column-count="3" same-size-horizontally="false" same-size-vertically="false" hgap="10" vgap="-1">
        <margin top="0" left="0" bottom="0" right="0"/>
        <constraints border-constraint="North"/>
        <properties>
          <background color="-12828863"/>
        </properties>
        <border type="none"/>
        <children>
          <component id="b0f6a" class="javax.swing.JLabel" binding="metricName">
            <constraints>
              <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="3" hsize-policy="3" anchor="0" fill="0" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties>
              <text value="Label"/>
            </properties>
          </component>
          <hspacer id="43bd0">
            <constraints>
              <grid row="0" column="1" row-span="1" col-span="1" vsize-policy="1" hsize-policy="6" anchor="0" fill="1" indent="0" use-parent-layout="false">
                <preferred-size width="53" height="11"/>
              </grid>
            </constraints>
          </hspacer>
          <component id="81ad4" class="javax.swing.JLabel" binding="metricScore">
            <constraints>
              <grid row="0" column="2" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="0" fill="0" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties>
              <text value="Label"/>
            </properties>
          </component>
        </children>
      </grid>
    </children>
  </grid>
</form>
